	FloatLayout:
		canvas:
			Color:
				rgb:(0.1,0.1,0.1)
			Rectangle:
				size:b20.size[0]*3,b20.size[1]*3
				pos:(0,0)
		BoxLayout:
			size:b20.size[0]*3,b20.size[1]*3
			pos:0,0
			BoxLayout:
				orientation:"vertical"
				Label:
					id::
					font_size: "20dp"
					font_name:'./Fonts/ARCADE.TTF'
					id:scrs
					text:"Score: 0"
					#on_state:print("my current state is {}".format(self.state))
				Label:
					id::
					font_size: "20dp"
					id: target
					font_name:'./Fonts/ARCADE.TTF'
					text:"Targate: 2500"

			BoxLayout:
				orientation:"vertical"
				FloatLayout:
					Label:
						text: "Power"
						pos: scrs.size[0],scrs.pos[1]
						font_name:'./Fonts/ARCADE.TTF'
						font_size: "20dp"
						size: scrs.size
					ProgressBar:
						id:pgpow
						max: 28
						size:scrs.size
						pos:scrs.pos[0]+scrs.size[0],scrs.pos[1] - scrs.pos[1]/2
				Button:
					font_size: "55dp"
					id:gobtn
					text:"GO"
					font_name:'./Fonts/ARCADE.TTF'
					on_press:root.pp(),root.fixpointloc()
					on_release:root.rollit()
			BoxLayout:
				orientation:"vertical"
				Label:
					font_size: "20dp"
					id:lev
					font_name:'./Fonts/ARCADE.TTF'
					text:"Level: 1"
				Label:
					font_size: "20dp"
					id:trys
					font_name:'./Fonts/ARCADE.TTF'
					text:"Chances: 10"