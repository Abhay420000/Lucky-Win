						pos: sc.size[0], sc.pos[1]
						font_name:'./Fonts/ARCADE.TTF'
						font_size: "30dp"
						size: scrs.size
					ProgressBar:
						id:pgpow
						max: 28
						size:target.size
						pos:target.size[0],target.pos[1] + target.size[1]/2
					Label:
						id:g2
						text: "GO"
						font_size: "31dp"
						font_name:'./Fonts/ARCADE.TTF'
						size: target.size
						pos: target.size[0], g3.pos[1] + self.size[1]/2
						color: (0,0,0)
				Label:
					id: g1
					text: "GO"
					font_name:'./Fonts/ARCADE.TTF'
					font_size: "24dp"
					color: (0,0,0)
				Label:
					id:g3
					text: "GO"
					font_name:'./Fonts/ARCADE.TTF'
					font_size: "38dp"
					color: (0,0,0)
				Button:
					font_size: "65dp"
					id:gobtn
					text:"GO"
					background_normal:""
					background_color:(0.2,0.4,1,0.5)
					color:(1,1,0)
					font_name:'./Fonts/ARCADE.TTF'
					on_press:root.pp(),root.fixpointloc()
					on_release:root.rollit()
			BoxLayout:
