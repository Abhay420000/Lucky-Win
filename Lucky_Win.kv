Main:

<DC@Widget>:
	size_hint:None,None
    size:(dp(100),dp(100))
    pos:(0,0)
    color:(1,1,1)

<DCF@DC>:
    canvas:
        Color:
            rgb:root.color
        Ellipse:
            pos:root.pos
            size:root.size


<Main>
