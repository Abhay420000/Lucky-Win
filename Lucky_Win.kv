			color:(0.13,0.13,0.13)
			size:root.size[0]/20,root.size[1]/2
			pos:root.size[0]-self.size[0]/2,root.size[1]/2

	FloatLayout:
		Image:
			source:"./DecImg/Bottom1.PNG"
			allow_stretch: True
			keep_ratio: False
			size:b20.size[0]*3,b20.size[1]*3
			pos:(0,0)
		BoxLayout:
			size:b20.size[0]*3,b20.size[1]*3
			pos:0,0
			BoxLayout:
				orientation:"vertical"
				Label:
					font_size: "30dp"
					font_name:'./Fonts/ARCADE.TTF'
					id:sc
					text:"Score"
					#on_state:print("my current state is {}".format(self.state))
					canvas.before:
						PushMatrix
						Rotate:
							angle: -12
							origin: self.center
					canvas.after:
						PopMatrix
				Label:
					font_size: "30dp"
					font_name:'./Fonts/ARCADE.TTF'
					id:scrs
					text:"0"
					canvas.before:
						PushMatrix
						Rotate:
							angle: -12
							origin: self.center
					canvas.after:
						PopMatrix
				Label:
					font_size: "30dp"
