Main:

<DC@Widget>:
	size_hint:None,None
    size:(dp(100),dp(100))
    pos:(0,0)
    color:(1,1,1)

<DCF@DC>:
    canvas:
        Color:
            rgb:root.color
        Ellipse:
            pos:root.pos
            size:root.size


<Main>
	orientation:"vertical"
	FloatLayout:
		size: root.size[0]/2, root.size[1]/2
		BoxLayout:
			size: root.size[0]/2, root.size[1]/2
			pos: 0,root.size[1]/2
			FloatLayout:
				AsyncImage:
					id:b0
					#nocache:True
					source:"./ImData/A.PNG"
					size_hint:(1,0.3333)
					pos:(0,root.height)
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b1
					source:"./ImData/B.PNG"
					size_hint:(1,0.3333)
					pos:(0,root.height - 1*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b2
					source:"./ImData/C.PNG"
					size_hint:(1,0.3333)
					pos:(0,root.height - 2*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b3
					source:"./ImData/D.PNG"
					size_hint:(1,0.3333)
					pos:(0,root.height - 3*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b4
					source:"./ImData/E.PNG"
					size_hint:(1,0.3333)
					pos:(0,root.height - 4*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b5
					source:"./ImData/F.PNG"
					size_hint:(1,0.3333)
					pos:(0,root.height - 5*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b6
					source:"./ImData/G.PNG"
					size_hint:(1,0.3333)
					pos:(0,root.height - 6*self.size[1])
					allow_stretch: True
					keep_ratio: False
			FloatLayout:
				AsyncImage:
					id:b7
					source:"./ImData/A.PNG"
					size_hint:(1,0.3333)
					pos:(self.size[0],root.height)
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b8
					source:"./ImData/B.PNG"
					size_hint:(1,0.3333)
					pos:(self.size[0],root.height - 1*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b9
					source:"./ImData/C.PNG"
					size_hint:(1,0.3333)
					pos:(self.size[0],root.height - 2*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b10
					source:"./ImData/D.PNG"
					size_hint:(1,0.3333)
					pos:(self.size[0],root.height - 3*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b11
					source:"./ImData/E.PNG"
					size_hint:(1,0.3333)
					pos:(self.size[0],root.height - 4*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b12
					source:"./ImData/F.PNG"
					size_hint:(1,0.3333)
					pos:(self.size[0],root.height - 5*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b13
					source:"./ImData/G.PNG"
					size_hint:(1,0.3333)
					pos:(self.size[0],root.height - 6*self.size[1])
					allow_stretch: True
					keep_ratio: False
			FloatLayout:
				AsyncImage:
					id:b14
					source:"./ImData/A.PNG"
					size_hint:(1,0.3333)
					pos:(2*self.size[0],root.height)
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b15
					source:"./ImData/B.PNG"
					size_hint:(1,0.3333)
					pos:(2*self.size[0],root.height - 1*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b16
					source:"./ImData/C.PNG"
					size_hint:(1,0.3333)
					pos:(2*self.size[0],root.height - 2*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b17
					source:"./ImData/D.PNG"
					size_hint:(1,0.3333)
					pos:(2*self.size[0],root.height - 3*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b18
					source:"./ImData/E.PNG"
					size_hint:(1,0.3333)
					pos:(2*self.size[0],root.height - 4*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b19
					source:"./ImData/F.PNG"
					size_hint:(1, 0.3333)
					pos:(2*self.size[0], root.height - 5*self.size[1])
					allow_stretch: True
					keep_ratio: False
				AsyncImage:
					id:b20
					source:"./ImData/G.PNG"
					size_hint:(1, 0.3333)
					pos:(2*self.size[0], root.height - 6*self.size[1])
					allow_stretch: True
					keep_ratio: False
		Widget:
			id:bgms
			size_hint:None,None
			size:root.size[0]/8, root.size[1]/25
			pos:root.size[0]/2 - self.size[0]/2, root.size[1] - self.size[1]
			canvas.before:
				Color:
					rgba:(0,0,1,0.5)#(1,0.61,1,0.5)
				RoundedRectangle:
					size:bgms.size
					pos:bgms.pos
					radius:[5]
		
		Widget:
			id:r1
			size_hint:None,None
			size:root.size[0], root.size[1]/6
			pos: 0, root.size[1] - self.size[1]
			canvas.before:
				Color:
					rgba: (1,0.61,1,0.5)
				Rectangle:
					size: r1.size
					pos: r1.pos
				Color:
					rgb: (0,0,0)
				Line:
					rectangle:(0, root.size[1] - self.size[1], root.size[0], root.size[1]/6)

		Widget:
			id:r2
			size_hint:None,None
			size:root.size[0], root.size[1]/6
			pos: 0, root.size[1]/2
			canvas.before:
				Color:
					rgba:(1,0.61,1,0.5)
				Rectangle:
					size:r2.size
					pos:r2.pos
				Color:
					rgb: (0,0,0)
				Line:
					rectangle:(0, root.size[1]/2, root.size[0], root.size[1]/6)

		
		BoxLayout:
			orientation:"horizontal"
			size_hint:None,None
			#pos_hint:None,None
			size: root.size[0]/8, root.size[1]/25
			pos: root.size[0]/2 - self.size[0]/2, root.size[1] - self.size[1]
			Button:
				id:bmof
				background_normal: ""
				background_color: (0,0,0,0)
				on_press: root.config_music()
				s: "./MSIco/music.png"
				AsyncImage:
					source: bmof.s
					size: bmof.size
					allow_stretch: True
					keep_ratio: True
					pos: bmof.pos
			Button:
				id:bsof
				background_normal: ""
				background_color: (0,0,0,0)
				s:"./MSIco/sound.png"
				on_press: root.config_sound()
				AsyncImage:
					source:bsof.s
					size: bsof.size
					allow_stretch: True
					keep_ratio: True
					pos: bsof.pos

		DCF:
			color:(0.13,0.13,0.13)
			size:root.size[0]/16,root.size[1]/2
			pos:-self.size[0]/2,root.size[1]/2
		DCF:
			color:(0.13,0.13,0.13)
			size:root.size[0]/16,root.size[1]/2
			pos:root.size[0]-self.size[0]/2,root.size[1]/2

	FloatLayout:
		Image:
			source:"./DecImg/Bottom1.PNG"
			allow_stretch: True
			keep_ratio: False
			size:b20.size[0]*3,b20.size[1]*3
			pos:(0,0)
		BoxLayout:
			size:b20.size[0]*3,b20.size[1]*3
			pos:0,0
			BoxLayout:
				orientation:"vertical"
				Label:
					font_size: "30dp"
					font_name:'./Fonts/ARCADE.TTF'
					id:sc
					text:"Score"
					#on_state:print("my current state is {}".format(self.state))
					canvas.before:
						PushMatrix
						Rotate:
							angle: -12
							origin: self.center
					canvas.after:
						PopMatrix
				Label:
					font_size: "30dp"
					font_name:'./Fonts/ARCADE.TTF'
					id:scrs
					text:"0"
					canvas.before:
						PushMatrix
						Rotate:
							angle: -12
							origin: self.center
					canvas.after:
						PopMatrix
				Label:
					font_size: "30dp"
					font_name:'./Fonts/ARCADE.TTF'
					text:"Targate"
					canvas.before:
						PushMatrix
						Rotate:
							angle: -12
							origin: self.center
					canvas.after:
						PopMatrix
				Label:
					font_size: "30dp"
					id: target
					font_name:'./Fonts/ARCADE.TTF'
					text:"250"
					canvas.before:
						PushMatrix
						Rotate:
							angle: -12
							origin: self.center
					canvas.after:
						PopMatrix

			BoxLayout:
				orientation:"vertical"
				FloatLayout:
					Label:
						text: "Power"
						pos: sc.size[0], sc.pos[1]
						font_name:'./Fonts/ARCADE.TTF'
						font_size: "30dp"
						size: scrs.size
					ProgressBar:
						id:pgpow
						max: 28
						size:target.size
						pos:target.size[0],target.pos[1] + target.size[1]/2
					Label:
						id:g2
						text: "GO"
						font_size: "31dp"
						font_name:'./Fonts/ARCADE.TTF'
						size: target.size
						pos: target.size[0], g3.pos[1] + self.size[1]/2
						color: (0,0,0)
				Label:
					id: g1
					text: "GO"
					font_name:'./Fonts/ARCADE.TTF'
					font_size: "24dp"
					color: (0,0,0)
				Label:
					id:g3
					text: "GO"
					font_name:'./Fonts/ARCADE.TTF'
					font_size: "38dp"
					color: (0,0,0)
				Button:
					font_size: "65dp"
					id:gobtn
					text:"GO"
					background_normal:""
					background_color:(0.2,0.4,1,0.5)
					color:(1,1,0)
					font_name:'./Fonts/ARCADE.TTF'
					on_press:root.pp(),root.fixpointloc()
					on_release:root.rollit()
			BoxLayout:
				orientation:"vertical"
				Label:
					font_size: "30dp"
					font_name:'./Fonts/ARCADE.TTF'
					text:"Level"
					canvas.before:
						PushMatrix
						Rotate:
							angle: +12
							origin: self.center
					canvas.after:
						PopMatrix
				Label:
					font_size: "30dp"
					id:lev
					font_name:'./Fonts/ARCADE.TTF'
					text:"1"
					canvas.before:
						PushMatrix
						Rotate:
							angle: +12
							origin: self.center
					canvas.after:
						PopMatrix
				Label:
					font_size: "30dp"
					font_name:'./Fonts/ARCADE.TTF'
					text:"Chances"
					canvas.before:
						PushMatrix
						Rotate:
							angle: +12
							origin: self.center
					canvas.after:
						PopMatrix
				Label:
					font_size: "30dp"
					id:trys
					font_name:'./Fonts/ARCADE.TTF'
					text:"10"
					canvas.before:
						PushMatrix
						Rotate:
							angle: +12
							origin: self.center
					canvas.after:
						PopMatrix